                        </span>

                    </div>

                    <div class="col-auto">
                        <!-- Footer LISTING social icon -->
                        <b:comment>share buttons</b:comment>
                        <div class="blog-listing-footer-inner-socialicons">
                            <ul>
                                <b:include cond='data:post.sharePostUrl or data:top.showEmailButton' data='post' name='shareButtons' />
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            </b:if>
            
            <div class='post-footer blog-listing-footer-outer'>
                <div class='post-footer-line post-footer-line-1'>
                    <span class='post-icons' style="position: absolute;right: 30px;">
                        <b:comment>quickedit pencil</b:comment>
                        <b:include data='post' name='postQuickEdit' />
                    </span>

                    
                    <b:if cond='data:view.isPost'>
                       <span class='reaction-buttons'>
                           <b:if cond='data:top.showReactions'>
                               <table cellpadding='0' cellspacing='0'>
                                   <tr>
                                       <td class='reactions-label-cell' nowrap='nowrap' style='vertical-align:top'>
                                           <span class='reactions-label'>
                                               <data:top.reactionsLabel/>
                                           </span>&#160;
                                       </td>
                                       <td>
                                           <iframe allowtransparency='true' class='reactions-iframe' expr:src='data:post.reactionsUrl' frameborder='0' name='reactions'
                                               scrolling='auto' />
                                       </td>
                                   </tr>
                               </table>
                           </b:if>
                       </span>
                    </b:if>

                    <span class='post-comment-link'>
                        <b:include cond='data:view.isMultipleItems and data:post.allowComments' data='post' name='comment_count_picker' />
                    </span>

                    <b:comment>backlinks</b:comment>
                    <span class='post-backlinks post-comment-link'>
                        <b:if cond='data:view.isMultipleItems and data:post.showBacklinks'>
                            <a class='comment-link' expr:href='data:post.url + "#links"'>
                                <data:top.backlinkLabel/>
                            </a>
                        </b:if>
                    </span>

                </div>
            </div>
        </div>
    </b:includable>
    
    <b:includable id='postQuickEdit' var='post'>
        <b:if cond='data:post.editUrl or true'>
            <span expr:class='"item-control " + data:post.adminClass'>
                <a expr:href='data:post.editUrl' expr:title='data:top.editPostMsg'>
                    <img alt='Post Quick Edit Icon' class='icon-action' height='18' src='https://resources.blogblog.com/img/icon18_edit_allbkg.gif'
                        width='18' />
                </a>
            </span>
        </b:if>
    </b:includable>
    
    <b:includable id='shareButtons' var='post'>
        <b:if cond='data:top.showFacebookButton'>
            <li>
               <a expr:title='data:top.shareToFacebookMsg' expr:href='&quot;https://www.facebook.com/sharer.php?u=&quot; + data:post.url + &quot;&amp;title=&quot;+ data:post.title' onclick='window.open(this.href, &apos;windowName&apos;, &apos;width=600, height=400, left=24, top=24, scrollbars, resizable&apos;); return false;' rel='nofollow' target='_blank' class="social-icon facebook">
                   <i class="fab fa-facebook-f"></i>
               </a>
            </li>
        </b:if>
        <b:if cond='data:top.showTwitterButton'>
            <li>
               <a expr:title='data:top.shareToTwitterMsg' expr:href='&quot;https://twitter.com/share?url=&quot; + data:post.url + &quot;&amp;title=&quot; + data:post.title' onclick='window.open(this.href, &apos;windowName&apos;, &apos;width=600, height=400, left=24, top=24, scrollbars, resizable&apos;); return false;' rel='nofollow' target='_blank' class="social-icon twitter">
                   <i class="fab fa-twitter"></i>
               </a>
            </li>
        </b:if>
        <b:if cond='data:top.showPinterestButton'>
            <li>
               <a expr:title='data:top.shareToPinterestMsg' expr:href='&quot;https://www.linkedin.com/shareArticle?url=&quot; + data:post.url + &quot;&amp;title=&quot;+ data:post.title' onclick='window.open(this.href, &apos;windowName&apos;, &apos;width=600, height=400, left=24, top=24, scrollbars, resizable&apos;); return false;' rel='nofollow' target='_blank' class="social-icon linkedin">
                   <i class="fab fa-linkedin-in"></i>
               </a>
            </li>
        </b:if>
        <b:if cond='data:post.emailPostUrl'>
            <li>
               <a expr:title='data:top.emailPostMsg' expr:href='data:post.emailPostUrl' rel='nofollow' class="social-icon googleplus">
                   <i class="fas fa-share-alt"></i>
               </a>
            </li>
        </b:if>
        <b:if cond='data:top.showPlusOne'>
            <div class='goog-inline-block google-plus-share-container'>
                <data:post.googlePlusShareTag/>
            </div>
        </b:if>
    </b:includable>
    
    <b:includable id='status-message'>
        <b:if cond='data:navMessage'>
            <div class='status-msg'>
                <data:navMessage/>
            </div>
            <script type='text/javascript'>
                $(".status-msg a[href='https://www.anphukhang.com.vn/']").attr('href', 'https://www.anphukhang.com.vn/search?&amp;max-results=7');
                $(".status-msg a").addClass( "goto" );
            </script>
            
        </b:if>
    </b:includable>
    
    <b:includable id='threaded-comment-form' var='post'>
        <div class='comment-form'>
            <a name='comment-form' />
            <b:if cond='data:mobile'>
                <p>
                    <data:blogCommentMessage/>
                </p>
                <data:blogTeamBlogMessage/>
